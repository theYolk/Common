{
	"plugins": [
		"plugins/markdown",
		"plugins/summarize"
	],
	"recurseDepth": 10,
	"source": {
		"include": ["./build"]
	},
	"sourceType": "module",
	"tags": {
		"allowUnknownTags": true,
		"dictionaries": ["jsdoc","closure"]
	},
	"templates": {
		"cleverLinks": false,
		"monospaceLinks": false
	},
	"opts": {
        "template": "./node_modules/docdash",
        "encoding": "utf8",
        "destination": "./docs/",
        "recurse": true
    }

}

